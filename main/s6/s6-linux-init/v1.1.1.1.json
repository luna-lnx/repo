{
    "git": "https://github.com/skarnet/s6-linux-init -b v1.1.1.1",
    "build": [
        "./configure",
        "make"
    ],
    "install": [
        "make install"
    ],
    "uninstall": [
        "rm -f /usr/lib/s6-linux-init/libs6_linux_init.a /usr/include/s6-linux-init/s6-linux-init.h /usr/include/s6-linux-init/config.h",
        "rm -f /etc/s6-linux-init/skel/rc.shutdown.final /etc/s6-linux-init/skel/rc.shutdown /etc/s6-linux-init/skel/runlevel /etc/s6-linux-init/skel/rc.init",
        "rm -f /bin/s6-linux-init-logouthookd /bin/s6-linux-init-hpr /bin/s6-linux-init-telinit /bin/s6-linux-init-echo /bin/s6-linux-init-shutdown /bin/s6-linux-init-shutdownd /bin/s6-linux-init-umountall /bin/s6-linux-init /bin/s6-linux-init-nuke /bin/s6-linux-init-maker"
    ],
    "depends": [
        "skalibs>=v2.13.1.1",
        "execline>=v2.9.3.0",
        "s6>=v2.11.3.2"
    ]
}