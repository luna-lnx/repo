#https://ftp.gnu.org/gnu/binutils/binutils-2.41.tar.gz
name = "binutils"
description = "binutils"
tag = "2.41"
tarball = "https://ftp.gnu.org/gnu/binutils/binutils-%s.tar.gz"
dependencies = [
    "kati"
]
build = [
    "chmod +x ./configure",
    "CC=$CC CXX=$CXX CFLAGS=$CFLAGS CXXFLAGS=$CFLAGS LDFLAGS=$LDFLAGS ./configure",
    "make $MKFLAGS"
]
install = [
    "make install DESTDIR=$DEST"
]