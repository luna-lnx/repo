#!/bin/sh

# Dependencies
export DEPENDS="make, clang"

# Git repo
export REPO="https://scm.osdn.net/gitroot/yash/yash.git"

# Git tag/branch
export TAG="2.55"

# Package name
# !THIS SHOULD NEVER CHANGE!
# !MUST BE THE SAME AS THE FILENAME!
export NAME="yash"

# Package description
export DESC="POSIX-compliant command line shell written in C99"

# Commands to build
export BUILD="make"

# Commands to install
export INSTALL="make install"